{
    "name": "tritrics\/tric-kirby",
    "description": "Plugin to use Kirby as content backend for Vue.js or any other webservice.",
    "version": "0.3.0",
    "authors": [
        {
            "name": "Michael Adams",
            "email": "ma@tritrics.dk"
        }
    ],
    "license": "ISC",
    "type": "kirby-plugin",
    "require": {
        "getkirby\/composer-installer": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Tritrics\\Tric\\v1\\": "lib\/",
            "Tritrics\\Tric\\v1\\Root\\": ".\/"
        },
        "files": [
            "config.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "getkirby\/composer-installer": true
        },
        "optimize-autoloader": true
    },
    "extra": {
        "installer-name": "tric"
    }
}